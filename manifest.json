{
  "name": "Remove Facebook News Feed",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Hide your facebook news feed, on the facebook homepage.",
  "default_locale": "en",
  "permissions": [
    "https://www.facebook.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}
